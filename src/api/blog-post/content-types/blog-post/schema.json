{
  "kind": "collectionType",
  "collectionName": "blog_posts",
  "info": {
    "singularName": "blog-post",
    "pluralName": "blog-posts",
    "displayName": "Blog Post",
    "description": ""
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "Title": {
      "type": "string"
    },
    "urlSlug": {
      "type": "string"
    },
    "Date": {
      "type": "date"
    },
    "BlogPostBody": {
      "type": "richtext"
    },
    "blog_authors": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "api::blog-author.blog-author",
      "mappedBy": "blog_posts"
    },
    "TextPreviewSnippet": {
      "type": "text"
    },
    "ListingCardImage": {
      "allowedTypes": [
        "images",
        "files",
        "videos",
        "audios"
      ],
      "type": "media",
      "multiple": false
    }
  }
}
